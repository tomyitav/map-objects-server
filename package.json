{
  "name": "@tomyitav/map-objects-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-schema": "rimraf ./dist/graphql/schema/ && dir /s /b \"src/graphql/types\" | findstr /e .ts > ts-files.txt && tsc ./src/graphql/schema/genereator-schema.ts --outDir ./dist/graphql/schema/ --lib esnext && tsc @ts-files.txt --outDir ./dist/graphql/types --lib esnext && del ts-files.txt",
    "generate-ts": "gql-gen --template ts --out ./src/interfaces/types.d.ts --schema ./dist/graphql/schema/genereator-schema.js",
    "pregenerate": "npm run build-schema",
    "generate": "npm run generate-ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run generate",
    "build": "tsc",
    "start": "sls offline start --skipCacheInvalidation",
    "dev": "tsc --watch & nodemon dist",
    "test": "tsc && mocha dist/**/*.spec.js",
    "lint": "eslint src --ext ts",
    "tsc": "tsc",
    "sls:deploy": "npm run build && sls deploy",
    "sls:offline": "npm run build && npm start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "graphql-server-express": "^1.4.0",
    "graphql-tools": "^4.0.2",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.7",
    "mongoose": "^5.3.7",
    "reflect-metadata": "^0.1.10",
    "serverless-http": "^1.6.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.3",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "2.2.41",
    "@types/mongoose": "^5.2.20",
    "@types/node": "7.0.22",
    "eslint": "4.0.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-promise": "3.5.0",
    "graphql-code-generator": "^0.12.5",
    "graphql-codegen-typescript-template": "^0.12.5",
    "mocha": "3.4.2",
    "nodemon": "1.11.0",
    "rimraf": "^2.6.2",
    "serverless-dotenv-plugin": "^2.0.1",
    "serverless-offline": "^3.25.14",
    "supertest": "3.0.0",
    "tsc": "1.20150623.0",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "3.0.0"
  }
}
